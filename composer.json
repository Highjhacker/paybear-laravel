{
    "name": "highjhacker/laravel-paybear",
    "description": "Laravel integration for PayBear",
    "keywords": ["laravel", "paybear", "cryptocurrency", "payment"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "highjhacker",
            "email": "jolanbeer@gmail.com",
            "homepage": "https://github.com/highjhacker"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "^5.7",
        "illuminate/database": "^5.7",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "Highjhacker\\Paybear\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Highjhacker\\Paybear\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Highjhacker\\Paybear\\PaybearServiceProvider"
            ]
        }
    }
}
